
@Model BisConnectivityServices.Models.AxAccessModel;
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                This is the main page of the Business connectivity. 
            </p>
        </div>
    </section>
}
    <section class="contact">
        <p>
            <header>
                <h3>This application is connected to:</h3>
            </header>
        <p>
            <ul>
                <li>
                    <span class="label">Tenant : </span>
                    <span>@Model.tentant</span>
                </li>
                <li>
                    <span class="label">Company : </span>
                    <span>@Model.company</span>
                </li>
                <li>
                    <span class="label">Uri : </span>
                    <span>@Model.uri</span>
                </li>
                <li>
                    <span class="label">Language : </span>
                    <span>@Model.language</span>
                </li>

            </ul>
        </p>
     </p>
        <p><a class="btn btn-default" href="~/biswebservice/ping" >Check connection &raquo;</a></p>

    </section>

